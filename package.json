    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.16.14",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "babel-preset-env": "^1.7.0",
    "eslint": "^8.39.0",
    "jsdom": "^22.0.0",
    "prettier": "^2.8.8",
